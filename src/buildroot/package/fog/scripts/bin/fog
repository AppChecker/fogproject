#!/bin/bash

### BEGIN FUNCTION INCLUDES
if [ -z "$FUNCS_SH" ]; then
	. /usr/share/fog/lib/funcs.sh;
fi
### END FUNCTION INCLUDES

if [ "$type" == "down" ]; then
	fog.download;
elif [ "$type" == "up" ]; then
	fog.upload;
else
	handleError " Fatal Error: Unknown request type :: Null";
fi
#fdisk -l &> /tmp/fdisk-after;
